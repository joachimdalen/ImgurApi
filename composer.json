{
  "name": "joachimdalen/imgur-api",
  "description": "An Imgur API wrapper for Laravel",
  "keywords": [
    "laravel",
    "upload",
    "imgur"
  ],
  "homepage": "https://github.com/joachimdalen/ImgurApi",
  "type": "library",
  "authors": [
    {
      "name": "Joachim Dalen",
      "email": "me@d3v.no"
    }
  ],
  "require": {
    "guzzlehttp/guzzle": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "JoachimDalen\\ImgurApi\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "JoachimDalen\\ImgurApi\\ImgurApiServiceProvider"
      ],
      "aliases": {
        "ImgurApi": "JoachimDalen\\ImgurApi\\ImgurApiFacade"
      }
    }
  },
  "minimum-stability": "dev"
}
